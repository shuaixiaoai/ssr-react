{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb-base",
    "rules": {
        "semi": [0],
        "import/no-extraneous-dependencies": [0],
        "indent": [1, 2],
        "no-unused-vars": [0],
        "react/jsx-filename-extension": [0]
    }
}